{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://tool-library.com/toolNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1
      },
      "catalogCode": {
        "type": "integer",
        "minimum": 0
      },
      "brand": {
        "type": "string"
      },
      "model": {
        "type": "string"
      },
      "condition": {
        "type": "string",
        "minLength": 1
      },
      "description": {
        "type": "string"
      },
      "contents": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
      "title",
      "catalogCode",
      "condition"
    ]
  }